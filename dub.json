{
	"description": "cairo bindings and wrappers for the D programming language.",
	"targetName": "cairoD",
	"authors": [
		"Johannes Pfau",
		"Andrej Mitrovic"
	],
	"version": "~master",
	"homepage": "https://github.com/jpf91/cairoD",
	"name": "cairod",
	"copyright": "Copyright Â© 2011-2013, Johannes Pfau",
	"license": "BSL-1.0",
	"-ddoxFilterArgs": ["--unittest-examples"],
	"configurations": [
		{
			"name": "static-lib-minimal",
			"targetType": "staticLibrary",
		},
		{
			"name": "static-lib-linux",
			"platforms": ["linux"],
			"targetType": "staticLibrary",
			"dependencies": {
				"x11": ">=1.0.1"
			}
		},
		{
			"name": "unittest",
			"targetType": "executable",
			"sourceFiles": ["unittest.d"],
			"libs": ["cairo"]
		}
	]
